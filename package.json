{
  "name": "soundkebap",
  "version": "1.0.0",
  "description": "An online music radio that connects to soundcloud and plays music you like",
  "main": "rethinkdb_init.js",
  "scripts": {
    "tsc": "tsc -p src -w",
    "tsc:w": "tsc -p src1 -w",
    "stylusCompile:watch": "stylus -w -u nib src1/assets/styles/main.styl",
    "lite": "lite-server --open hero_tutorial",
    "start": "concurrent \"npm run tsc:w\" \"npm run stylusCompile:watch\" \"node app.js\"",
    "dev": "node app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sittenstrolch/Soundkebap.git"
  },
  "author": "Sebastian Rehfeldt, Markus Petrykowski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sittenstrolch/Soundkebap/issues"
  },
  "homepage": "https://github.com/Sittenstrolch/Soundkebap#readme",
  "dependencies": {
    "angular2": "^2.0.0-beta.1",
    "body-parser": "^1.14.2",
    "bootstrap": "^3.3.6",
    "cookie-parser": "^1.4.1",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "express": "^4.13.3",
    "font-awesome": "^4.5.0",
    "ng2-cookies": "^0.1.2",
    "nib": "^1.1.0",
    "node-soundcloud": "0.0.4",
    "path": "^0.12.7",
    "plangular": "0.0.1",
    "q": "^1.4.1",
    "reflect-metadata": "0.1.2",
    "rethinkdb": "^2.2.1",
    "rxjs": "5.0.0-beta.0",
    "soundcloud": "^3.0.1",
    "stylus": "^0.53.0",
    "systemjs": "0.19.6",
    "zone.js": "0.5.10"
  },
  "devDependencies": {
    "concurrently": "^1.0.0",
    "lite-server": "^1.3.1",
    "typescript": "^1.7.3"
  }
}
